                    </nav>
                    <div data-tabs-body class="tabs__content">
                        <div class="tabs__body">
                            <form action="#" class="form">
                                <div class="form__content">
                                    <div class="form__inputs">
                                        <div class="form__input">
                                            <input data-required="email" autocomplete="off" type="text" name="form[]"
                                                data-error="Обязательное поле" placeholder="Email" class="input">
                                        </div>
                                        <div class="form__input">
                                            <input data-required data-error="Обязательное поле" autocomplete="off"
                                                type="password" name="form[]" placeholder="Пароль">
