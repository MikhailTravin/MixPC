                            </form>
                            <a href="#" data-popup="#forgot-password" class="forgot-password">
                                <span>Забыли пароль?</span>
                            </a>
                        </div>
                        <div class="tabs__body">
                            <form action="#" class="form">
                                <div class="form__content">
                                    <div class="form__inputs">
                                        <div class="form__input">
                                            <input data-required autocomplete="off" type="text" name="form[]"
                                                data-error="Обязательное поле" placeholder="Ваше имя" class="input">
                                        </div>
                                        <div class="form__input">
                                            <input data-required="email" autocomplete="off" type="text" name="form[]"
                                                data-error="Обязательное поле" placeholder="Номер телефона"
                                                class="input">
                                        </div>
                                        <div class="form__input">
                                            <input data-required autocomplete="off" type="text" name="form[]"
                                                data-error="Обязательное поле" placeholder="Ваше имя" class="input">
                                        </div>
                                        <div class="form__input">
                                            <input id="password" data-required data-error="Обязательное поле"
                                                autocomplete="off" type="password" name="form[]" placeholder="Пароль">
                                            <div class="form__viewpass">
                                                <img class="eye1" loading="lazy" src="../img/icon/eye2.svg" alt="">
